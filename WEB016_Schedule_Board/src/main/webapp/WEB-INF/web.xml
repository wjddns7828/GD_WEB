<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
	
	<display-name>Web Application</display-name>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>CalController</servlet-name>
		<servlet-class>com.min.edu.ctrl.CalController</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>CalController</servlet-name>
		<url-pattern>/CalController.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CalListAjax</servlet-name>
		<servlet-class>com.min.edu.api.CalListAjax</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>CalListAjax</servlet-name>
		<url-pattern>/CalListAjax.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>MultiDelForm</servlet-name>
		<servlet-class>com.min.edu.ctrl.MultiDelForm</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>MultiDelForm</servlet-name>
		<url-pattern>/DelController.do</url-pattern>
	</servlet-mapping>
	
</web-app>