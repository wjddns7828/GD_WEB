<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
	
	<display-name>web004_myBatis_Join_Training</display-name>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<!-- httpServelet을 extends하고 있는 객체 -->
	<servlet>
		<servlet-name>mybatisjoin</servlet-name>
		<servlet-class>com.min.edu.ctrl.MyBatisJoinController</servlet-class>
	</servlet>
	<!-- servletClass를 호출 할 주소 -->
	<servlet-mapping>
			<servlet-name>mybatisjoin</servlet-name>
			<url-pattern>/joinCtrl.do</url-pattern>
	</servlet-mapping>
</web-app>