<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
	
	<display-name>Web Application</display-name>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>fileboard</servlet-name>
		<servlet-class>com.min.edu.ctrl.FileController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>fileboard</servlet-name>
		<url-pattern>/fileboard.do</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>imgPreview</servlet-name>
		<servlet-class>com.min.edu.ctrl.ImgPreview</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>imgPreview</servlet-name>
		<url-pattern>/imgPreview.do</url-pattern>
	</servlet-mapping>
</web-app>